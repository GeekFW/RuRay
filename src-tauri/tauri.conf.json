{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "RuRay",
  "version": "1.0.5",
  "identifier": "com.lander.ruray",
  "build": {
    "beforeBuildCommand": "pnpm generate",
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../.output/public"
  },
  "app": {
    "windows": [
      {
        "title": "RuRay",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": false,
        "transparent": true,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "wintun/bin/amd64/wintun.dll",
      "wintun/bin/x86/wintun.dll",
      "wintun/bin/arm64/wintun.dll",
      "wintun/bin/arm/wintun.dll",
      "tun2proxy/*",
      "default.pac"
    ]
  },
  "plugins": {}
}
